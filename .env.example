# Database URL
# For local development with Prisma Postgres:
DATABASE_URL="prisma+postgres://localhost:51213/?api_key=YOUR_API_KEY_HERE"

# For production (Vercel Postgres, Neon, Supabase, etc.):
# DATABASE_URL="postgresql://user:password@host:port/database?sslmode=require"

# Admin password for /admin access
# IMPORTANT: Change this to a strong password in production!
ADMIN_PASSWORD="your-secure-password-here"

# Resend API key for email notifications
RESEND_API_KEY="re_your_api_key_here"
